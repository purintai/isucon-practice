<h2>あしあとリスト</h2>
<div class="row panel panel-primary" id="footprints">
  <ul class="list-group">
    <% footprints.each do |id, updated_at| %>
    <% owner = get_user(id) %>
    <li class="list-group-item footprints-footprint"><%= Time.at(updated_at) %>: <a href="/profile/<%= owner[:account_name] %>"><%= owner[:nick_name] %>さん</a>
    <% end %>
  </ul>
</div>
